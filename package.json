{
  "name": "labyrinth",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "mix:build": "cross-env NODE_ENV=production webpack --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix:watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build:renderer": "cross-env MIX_TARGET=renderer yarn mix:build",
    "watch:renderer": "cross-env MIX_TARGET=renderer yarn mix:watch",
    "build:main": "cross-env MIX_TARGET=main yarn mix:build",
    "watch:main": "cross-env MIX_TARGET=main yarn mix:watch",
    "build": "yarn build:renderer && yarn build:main",
    "start": "electron ./build/main.js",
    "dist:dir": "electron-builder --dir",
    "dist:pkg": "electron-builder"
  },
  "main": "build/main.js",
  "postinstall": "electron-builder install-app-deps",
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron"
  ],
  "author": "GitHub",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@emotion/is-prop-valid": "^0.8.2",
    "@maxmind/geoip2-node": "^0.6.0",
    "@types/filesize": "^4.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/recharts": "^1.1.17",
    "@types/tar-stream": "^1.6.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cross-env": "^5.2.0",
    "electron": "^6.0.0-beta.12",
    "electron-builder": "^20.44.4",
    "electron-is": "^3.0.0",
    "filesize": "^4.1.2",
    "fs-extra": "^8.1.0",
    "ionicons": "^4.5.8",
    "js-yaml": "^3.13.1",
    "laravel-mix": "^4.0.16",
    "node-sass": "^4.12.0",
    "postcss-easing-gradients": "^3.0.1",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "resolve-url-loader": "2.3.1",
    "sass-loader": "^7.1.0",
    "tar-stream": "^2.1.0",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {}
}
